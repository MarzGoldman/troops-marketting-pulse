    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "fastify": "^4.29.1",
    "jose": "^6.1.0",
    "nanoid": "^5.1.6",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "tsx": "^4.16.0",
    "typescript": "^5.6.2"
  }
}